{
  "development": {
    "service": {
      "base_url": "ui"
    },
    "flask": {
      "host": "0.0.0.0",
      "port": 8002,
      "debug": true,
      "use_reloader": false
    },
    "errors": {
      "show_exceptions": true
    },
    "logging": {
      "level": "DEBUG",
      "style": "{",
      "datefmt": "%Y-%m-%d :: %Z[%z] :: %H:%I:%S",
      "format": "{asctime} :: PID: {process} :: {name} :: {levelname} :: {message}"
    }
  },
  "production": {
    "service": {
      "base_url": "ui"
    },
    "flask": {
      "host": "0.0.0.0",
      "port": 80,
      "debug": false,
      "use_reloader": false
    },
    "errors": {
      "show_exceptions": false
    },
    "logging": {
      "level": "ERROR",
      "style": "{",
      "datefmt": "%Y-%m-%d :: %Z[%z] :: %H:%I:%S",
      "format": "{asctime} :: PID: {process} :: {name} :: {levelname} :: {message}"
    }
  }
}
